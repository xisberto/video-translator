{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:*:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "rekognition:DetectLabels",
        "rekognition:StartLabelDetection",
        "rekognition:GetLabelDetection",
        "transcribe:StartTranscriptionJob",
        "translate:StartTextTranslationJob",
        "iam:PassRole",
        "s3:GetObject",
        "s3:PutObject",
        "s3:GetObjectAttributes"
      ],
      "Resource": "*"
    }
  ]
}